execution_count":28}]},{"cell_type":"code","source":["def check_confidence_val(text):\n","  text = str(text)\n","  text = re.sub('@[^\\s]+','',text)\n","  text = re.sub(r'http\\S+', '', text)\n","  text = re.sub('#[^\\s]+','',text)\n","\n","  # Define the regex pattern\n","  url_pattern = re.compile(r'\\b(?:https?://|www\\.)\\S+\\b|\\b(?:\\S+\\.)?(?!http://|www\\.)\\S+\\.\\S+\\b', re.IGNORECASE)\n","\n","  # Remove URLs that don't start with \"http://\" or \"www.\"\n","  text = re.sub(url_pattern, '', text)\n","\n","  confidence = detector.compute_language_confidence(text, Language.INDONESIAN)\n","  return f\"{confidence:.4f}\""],"metadata":{"id":"UO0UzFbhS4M-","executionInfo":{"status":"ok","timestamp":1704268146772,"user_tz":-420,"elapsed":2,"user":{"displayName":"Synithis","userId":"03957030646426939799"}}},"execution_count":30,"outputs":[]},{"cell_type":"code","source":["df_merge_nonid['lang'] = df_merge_nonid['text'].apply(lambda x: get_lang_df(x))\n","df_merge_nonid['conf_score'] = df_merge_nonid['text'].apply(lambda x: check_confidence_val(x))\n","df_merge_nonid['conf_score'] = df_merge_nonid['conf_score'].astype(float)\n","df_merge_nonid\n","df_merge_nonid.to_excel('report/'+platform+'/merge_nonid_data_komen_ultra_conf_val.xlsx')"],"metadata":{"id":"uSAayqkuS5li","executionInfo":{"status":"ok","timestamp":1704268328202,"user_tz":-420,"elapsed":4858,"user":{"displayName":"Synithis","userId":"03957030646426939799"}}},"execution_count":32,"outputs":[]},{"cell_type":"code","source":["df_merge_nonid_conf = df_merge_nonid[df_merge_nonid['conf_score'] >= 0.5]\n","df_merge_nonid_conf.reset_index(drop=True, inplace=True)\n","df_merge_nonid_conf.drop(columns=['conf_score'], inplace=True)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"iiUWFClrars0","executionInfo":{"status":"ok","timestamp":1704270573276,"user_tz":-420,"elapsed":309,"user":{"displayName":"Synithis","userId":"03957030646426939799"}},"outputId":"291012ef-af52-47db-877b-35a16e3b2d4f"},