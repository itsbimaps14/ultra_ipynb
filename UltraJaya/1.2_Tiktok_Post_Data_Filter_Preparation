a_komen['bookmark'] = \"NULL\"\n","  df_data_komen['quotes'] = \"NULL\"\n","  df_data_komen['link'] = data_post\n","\n","  df_data_komen = df_data_komen[kolom]\n","\n","  # Remove Username\n","  df_data_komen['text'] = df_data_komen['text'].apply(lambda x: remove_username(x))\n","\n","  # result = [get_language(x) for x in df_data_komen['text']]\n","  df_data_komen['lang'] = df_data_komen['text'].apply(lambda x: get_lang_df(x))\n","\n","  df_data_komen.reset_index(drop=True, inplace=True)\n","\n","  df_komen_id = df_data_komen[df_data_komen['lang'] == Language.INDONESIAN]\n","  df_komen_id.reset_index(drop=True, inplace=True)\n","\n","  df_komen_non_id = df_data_komen[df_data_komen['lang'] != Language.INDONESIAN]\n","  df_komen_non_id.reset_index(drop=True, inplace=True)\n","\n","  return df_data_komen, df_komen_id, df_komen_non_id"],"metadata":{"id":"5B8S7QODmZJX","executionInfo":{"status":"ok","timestamp":1704267564279,"user_tz":-420,"elapsed":12,"user":{"displayName":"Synithis","userId":"03957030646426939799"}}},"execution_count":23,"outputs":[]},{"cell_type":"code","source":["df_merge_all = pd.DataFrame(columns=kolom)\n","df_merge_id = pd.DataFrame(columns=kolom)\n","df_merge_nonid = pd.DataFrame(columns=kolom)"],"metadata":{"id":"c1H1tbU3sy2s","executionInfo":{"status":"ok","timestamp":1704267564279,"user_tz":-420,"elapsed":12,"user":{"displayName":"Synithis","userId":"03957030646426939799"}}},"execution_count":24,"outputs":[]},{"cell_type":"code","source":["for brand in brands :\n","  for post in posts[brand]:\n","\n","    hashtag, kode_post = post.split('-')\n","\n","    # Read file into a list\n","    with open('data_scraped/'+platform+'/'+type_scrap+'/'+brand+'/'+post+'.txt', 'r') as f:\n","        lines = f.readlines()\n","\n","    df_all, df_id, df_non_id = transform_data(lines, brand, hashtag, post)\n","\n","    df_merge_all = pd.concat([df_merge_all, df_all])\n","    df_merge_id = pd.concat([df_merge_id, df_id])\n","    df_merge_nonid = pd.concat([df_me